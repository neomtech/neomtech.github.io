<script>
    $(document).ready(function() {
      //smooth-scroll
      $('.nav-button').click(function(event) {
        $("#header").addClass("header_fixed");
        event.preventDefault();
        var link = this;
        $.smoothScroll({
          scrollTarget: link.hash
        });
      });

      $('button.scrollsomething').click(function() {
        $.smoothScroll({
          scrollElement: $('.scrollme'),
          scrollTarget: '#findme'
        });
        return false;
      });

      //FlexSlider
      $('.flexslider').flexslider({
        animation: "slide",
        start: function(slider){
          $('body').removeClass('loading');
        }
      });

      // show staff layer
      $(".item a").click(function() {
        $('<div class="overlay"></div>').appendTo('body');
        $('#staffShow .slides>li').hide();
        var lr_trg = $(this).parent().attr('class');
        var lr_trg = '#' + lr_trg.substr((lr_trg.length-6));
        $('#staffShow').fadeIn('fast', function () {
            $(lr_trg).fadeIn('fast');
        });
        return false;
      });

      // hide staff layer
      $('#staffShow .layer_close a, .overlay').live("click", function(){
        $('#staffShow').hide();
        $('.overlay').remove();
        return false;
      });

      // nav fixed
      var initial_offset_top = $("#header").offset().top;
      $(window).scroll(function() {
          if ((document.documentElement.scrollTop + document.body.scrollTop) > initial_offset_top) {
              $("#header").addClass("header_fixed");
          } else {
              $("#header").removeClass("header_fixed");
          }
      });
    });
  </script>


